{
  "name": "kit",
  "version": "3.2.0",
  "description": "a simple client-side development setup",
  "scripts": {
    "dev": "webpack-dev-server --env.development",
    "build": "webpack --env.production",
    "image": "docker build -t zacharykane/kit .",
    "docker-dev": "docker run -it -p 8080:8080 --rm -v ~/Documents/GitHub/kit/:/app -v /app/node_modules zacharykane/kit dev",
    "docker-build": "docker run -it -p 8080:8080 --rm -v ~/Documents/GitHub/kit/:/app -v /app/node_modules zacharykane/kit build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacharykane/kit.git"
  },
  "keywords": [
    "starter",
    "development",
    "client",
    "es2015"
  ],
  "author": "zacharykane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zacharykane/kit/issues"
  },
  "homepage": "https://github.com/zacharykane/kit#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.4.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3"
  },
  "dependencies": {
    "moment": "^2.19.1",
    "normalize.css": "^7.0.0"
  }
}

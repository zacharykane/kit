{
  "name": "kit",
  "version": "4.0.0",
  "description": "a simple client-side development setup",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.config.development.js",
    "build": "webpack --config webpack.config.production.js",
    "test": "webpack --config webpack.config.test.js && open ./public/dist/test.html",
    "lint": "eslint src/js & stylelint src/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacharykane/kit.git"
  },
  "keywords": [
    "starter",
    "development",
    "client"
  ],
  "author": "zacharykane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zacharykane/kit/issues"
  },
  "homepage": "https://github.com/zacharykane/kit#readme",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "1.18.2",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "tape": "^4.11.0",
    "terser-webpack-plugin": "^2.0.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "normalize.css": "^8.0.0"
  }
}

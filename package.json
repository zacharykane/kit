{
  "name": "kit",
  "version": "4.0.0",
  "description": "a simple client-side development setup",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.config.development.js",
    "build": "webpack --config webpack.config.production.js",
    "test": "webpack --config webpack.config.test.js && open ./public/dist/test.html",
    "lint": "eslint src/js & stylelint src/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacharykane/kit.git"
  },
  "keywords": [
    "starter",
    "development",
    "client"
  ],
  "author": "zacharykane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zacharykane/kit/issues"
  },
  "homepage": "https://github.com/zacharykane/kit#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "1.17.1",
    "style-loader": "^0.23.0",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0",
    "tape": "^4.10.2",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "normalize.css": "^8.0.0"
  }
}

{
  "name": "kit",
  "version": "4.0.0",
  "description": "a simple client-side development setup",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.config.development.js",
    "build": "webpack --config webpack.config.production.js",
    "test": "webpack --config webpack.config.test.js && open ./public/dist/test.html",
    "lint": "eslint src/js & stylelint src/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacharykane/kit.git"
  },
  "keywords": [
    "starter",
    "development",
    "client"
  ],
  "author": "zacharykane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zacharykane/kit/issues"
  },
  "homepage": "https://github.com/zacharykane/kit#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.0.7",
    "prettier": "1.14.3",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended": "^2.1.0",
    "tape": "^4.9.1",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "normalize.css": "^8.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}

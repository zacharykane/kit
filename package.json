{
  "name": "kit",
  "version": "3.1.0",
  "description": "a simple client-side development setup",
  "scripts": {
    "start-docker": "docker run --name kit -p 8080:8080 -v %cd%:/usr/src/app zacharykane/kit",
    "start": "cross-env NODE_ENV=development webpack-dev-server --host=0.0.0.0 --env=dev --open",
    "build": "rimraf public/dist && cross-env NODE_ENV=production webpack --env=prod --progress --colors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacharykane/kit.git"
  },
  "keywords": [
    "starter",
    "development",
    "client",
    "es2015"
  ],
  "author": "zacharykane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zacharykane/kit/issues"
  },
  "homepage": "https://github.com/zacharykane/kit#readme",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.2.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-loader": "^1.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "moment": "^2.18.1"
  }
}

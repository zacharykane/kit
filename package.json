{
  "name": "kit",
  "version": "3.3.0",
  "description": "a simple client-side development setup",
  "scripts": {
    "dev": "webpack-dev-server --env.development",
    "build": "webpack --env.production",
    "build-test": "webpack --env.testing",
    "test": "babel-tape-runner src/**/*.test.js | faucet",
    "image": "docker build -t zacharykane/kit .",
    "docker-dev":
      "docker run -it -p 8080:8080 --rm -v ~/Documents/GitHub/kit/:/app -v /app/node_modules zacharykane/kit dev",
    "docker-build":
      "docker run -it -p 8080:8080 --rm -v ~/Documents/GitHub/kit/:/app -v /app/node_modules zacharykane/kit build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacharykane/kit.git"
  },
  "keywords": ["starter", "development", "client", "es2017"],
  "author": "zacharykane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zacharykane/kit/issues"
  },
  "homepage": "https://github.com/zacharykane/kit#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-tape-runner": "^2.0.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.8",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "faucet": "0.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.9.2",
    "prettier-eslint": "^8.8.1",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "tape": "^4.8.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "dependencies": {
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "regenerator": "^0.12.3",
    "tap-html": "^0.2.0"
  },
  "browserslist": ["> 1%", "last 2 versions"]
}

{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "prettier/prettier": ["error", { "singleQuote": true, "tabWidth": 4, "printWidth": 150 }]
  },
  "plugins": ["prettier"]
}
